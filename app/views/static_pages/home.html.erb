<% if logged_in? %>
  <div class='row'>
    <div class='col-xs-8 col-xs-offset-2 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-3'>
      <h1>News</h1>
      <%= link_to "New post", new_post_path %>
      <%= render 'shared/feed' %>
    </div>
  </div>
<% else %>
<div class='center jumbotron'>
    <%= image_tag('sbcuervos_logo.png', height: 200, width: 200) %>
  <br/><br/>
    Don't have an account? <strong><%= link_to "Sign up now!", signup_path%></strong>
</div>
<div class="row">
  <div class='col-xs-8 col-xs-offset-2 col-sm-8 col-sm-offset-2 col-md-6 col-md-offset-3 col-lg-6 col-lg-offset-3'>
    <h1>News</h1>
    <%= render 'shared/feed' %>
  </div>
</div>
<% end %>